{
  "compilerOptions": {
    "target": "ES2022",
    "module": "NodeNext",
    "rootDir": "./src",
    // "moduleResolution": "node10",
    "moduleResolution": "nodenext",
    "outDir": "./dist",
    "esModuleInterop": true,
     "isolatedModules": true,
    "forceConsistentCasingInFileNames": true, 
    /* Type Checking */
    "strict": true,
    "skipLibCheck": true ,
  "types": ["node", "k6"]
  },
  "include": [
    "src/**/*" , 
     "load.tests"
  ],
  "exclude": [
    "node_modules",
    "**/*.spec.ts"
  ],
}
